---
import Layout from "@/layouts/Layout.astro";
import Mv from "@/components/pages/home/mv/Mv.astro";
import Concern from "@/components/pages/home/concern/Concern.astro";
import Demo from "@/components/pages/home/demo/Demo.astro";
import Feature from "@/components/pages/home/feature/Feature.astro";
import Plan from "@/components/pages/home/plan/Plan.astro";
import Flow from "@/components/pages/home/flow/Flow.astro";
import Faq from "@/components/pages/home/faq/Faq.astro";
